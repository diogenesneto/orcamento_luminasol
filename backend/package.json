{
  "name": "luminasol-backend",
  "version": "1.0.0",
  "description": "Sistema de OrÃ§amentos Solares - Backend API",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "docs": "apidoc -i src/ -o docs/"
  },
  "keywords": [
    "solar",
    "budget",
    "energy",
    "luminasol",
    "api"
  ],
  "author": "LuminaSol Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "sequelize": "^6.35.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.7",
    "multer": "^1.4.5-lts.1",
    "winston": "^3.11.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "crypto": "^1.0.1",
    "pizzip": "^3.1.4",
    "docxtemplater": "^3.42.0",
    "exceljs": "^4.4.0",
    "ua-parser-js": "^1.0.37"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "sequelize-cli": "^6.6.2",
    "apidoc": "^1.0.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,md}\"",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:reset": "npm run db:migrate:undo && npm run db:migrate && npm run db:seed",
    "logs": "tail -f logs/app.log",
    "logs:error": "tail -f logs/error.log",
    "docs": "apidoc -i src/ -o docs/",
    "docker:build": "docker build -t luminasol-backend .",
    "docker:run": "docker run -p 3001:3001 luminasol-backend"
  }
}
